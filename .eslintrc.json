{
    "extends": [
        "trigo/react-typescript",
        "prettier",
        "prettier/react",
        "prettier/@typescript-eslint"
    ],
    "plugins": [
        "prettier"
    ],
    "env": {
        "node": true,
        "browser": true,
        "jest": true
    },
    "rules": {
        "prettier/prettier": [
            "warn"
        ],
        "@typescript-eslint/no-explicit-any": 0,
        "import/no-named-as-default": 0,
        "import/extensions": 0,
        "react/jsx-filename-extension": [0]
        }
}
