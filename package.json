{
  "name": "@trigo/design-system",
  "version": "13.1.0",
  "description": "trigo design system",
  "main": "dist/designsystem.cjs.js",
  "module": "dist/designsystem.esm.js",
  "sideEffects": false,
  "repository": "git@github.com:trigo-at/design-system.git",
  "author": "trigo GmbH <office@trigo.at>",
  "license": "MIT",
  "engines": {
    "node": ">=13.0.0"
  },
  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "prettify": "prettier --write '**/*.{js,jsx}'",
    "test": "NODE_ENV=test jest --coverage --setupTestFrameworkScriptFile=./jest.setup.js",
    "build": "NODE_ENV=production rollup -c",
    "watch": "rollup -c -w",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o docs --docs",
    "prepare": "rm -rf ./dist && npm run build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.35",
    "@reach/router": "^1.3.4",
    "@storybook/addon-a11y": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/addons": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@storybook/theming": "^6.0.26",
    "@tailwindcss/ui": "^0.7.2",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "autoprefixer": "^9.8.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-emotion": "^10.0.33",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-react-app": "^9.1.2",
    "chance": "^1.1.7",
    "core-js": "3.8.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.16.1",
    "emotion-theming": "^10.0.27",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-trigo": "^15.0.11",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "final-form": "^4.20.1",
    "final-form-arrays": "^3.0.2",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "postcss-import": "^14.0.0",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-final-form": "6.5.2",
    "react-final-form-arrays": "3.1.3",
    "react-intl": "5.10.9",
    "regenerator-runtime": "^0.13.7",
    "rollup": "^2.35.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "tailwindcss": "^1.8.10",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@chakra-ui/core": "^0.8",
    "@emotion/core": "^10",
    "@reach/router": "^1",
    "core-js": "^3.8.1",
    "date-fns": "^2",
    "final-form": "^4",
    "final-form-arrays": "^3",
    "prop-types": "^15",
    "react": "^16",
    "react-final-form": "^6.5.2",
    "react-final-form-arrays": "^3.1.3",
    "react-intl": "^5.10.9"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)(spec|test|specs).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/?(*.)(stories).js?(x)"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
