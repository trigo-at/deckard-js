{
  "name": "@trigo/design-system",
  "version": "4.0.0",
  "description": "trigo design system",
  "main": "dist/designsystem.cjs.js",
  "module": "dist/designsystem.esm.js",
  "sideEffects": false,
  "repository": "git@github.com:trigo-at/design-system.git",
  "author": "trigo GmbH <office@trigo.at>",
  "license": "MIT",
  "engines": {
    "node": ">=9.0.0"
  },
  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "prettify": "prettier --write '**/*.{js,jsx}'",
    "test": "NODE_ENV=test jest --coverage --setupTestFrameworkScriptFile=./jest.setup.js",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o docs",
    "prepare": "rm -rf ./dist && npm run build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
      "eslint .",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@chakra-ui/core": "^0.5.0",
    "@emotion/core": "^10.0.22",
    "@reach/router": "^1.2.1",
    "@storybook/addon-a11y": "^5.2.8",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-docs": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@storybook/theming": "^5.2.8",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.23",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chance": "^1.1.4",
    "core-js": "^3.5.0",
    "cross-env": "^6.0.3",
    "date-fns": "^2.8.1",
    "emotion-theming": "^10.0.19",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-trigo": "^12.0.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "final-form": "^4.18.6",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-final-form": "^6.3.3",
    "react-intl": "^3.9.2",
    "regenerator-runtime": "^0.13.3",
    "rollup": "^1.27.11",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.3"
  },
  "peerDependencies": {
    "prop-types": "^15",
    "react": "^16",
    "final-form": "^4",
    "react-final-form": "^6",
    "react-intl": "^3",
    "@reach/router": "^1",
    "@chakra-ui/core": "^0.5",
    "@emotion/core": "^10",
    "date-fns": "^2"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)(spec|test|specs).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/?(*.)(stories).js?(x)"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
