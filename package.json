{
  "name": "@trigo/design-system",
  "version": "14.11.2",
  "description": "trigo design system",
  "main": "dist/designsystem.cjs.js",
  "module": "dist/designsystem.esm.js",
  "sideEffects": false,
  "repository": "git@github.com:trigo-at/design-system.git",
  "author": "trigo GmbH <office@trigo.at>",
  "license": "MIT",
  "engines": {
    "node": ">=13.0.0"
  },
  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "prettify": "prettier --write '**/*.{js,jsx}'",
    "test": "NODE_ENV=test jest --coverage --setupTestFrameworkScriptFile=./jest.setup.js",
    "build": "NODE_ENV=production rollup -c",
    "watch": "rollup -c -w",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o docs --docs",
    "prepublishOnly": "rm -rf ./dist && npm run build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/eslint-plugin": "^7.13.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@chakra-ui/react": "1.3.4",
    "@emotion/babel-plugin": "^11.2.0",
    "@emotion/react": "11.1.5",
    "@emotion/styled": "^11.1.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@storybook/addon-a11y": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/addons": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@storybook/theming": "^6.1.21",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.5",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "autoprefixer": "^10.2.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-react-app": "^10.0.0",
    "chance": "^1.1.7",
    "core-js": "3.9.1",
    "cross-env": "^7.0.3",
    "date-fns": "2.19.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-trigo": "^18.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.2.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "final-form": "4.20.2",
    "final-form-arrays": "^3.0.2",
    "framer-motion": "^3.10.3",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "^17.0.1",
    "react-final-form": "6.5.2",
    "react-final-form-arrays": "3.1.3",
    "react-intl": "5.13.2",
    "regenerator-runtime": "^0.13.7",
    "rollup": "^2.41.1",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^1.3.4",
    "@emotion/react": "^11",
    "core-js": "^3.9.1",
    "date-fns": "^2.19.0",
    "final-form": "^4.20.2",
    "final-form-arrays": "^3",
    "prop-types": "^15",
    "react": "^17",
    "react-final-form": "^6",
    "react-final-form-arrays": "^3",
    "react-intl": "^5.13.2"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)(spec|test|specs).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/?(*.)(stories).js?(x)"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@storybook/addon-actions": "^6.1.21",
    "react-table": "^7.6.3"
  }
}
