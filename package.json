{
  "name": "@trigo/design-system",
  "version": "15.7.4",
  "description": "trigo design system",
  "main": "dist/designsystem.cjs.js",
  "module": "dist/designsystem.esm.js",
  "sideEffects": false,
  "repository": "git@github.com:trigo-at/design-system.git",
  "author": "trigo GmbH <office@trigo.at>",
  "license": "MIT",
  "engines": {
    "node": ">=13.0.0"
  },
  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "prettify": "prettier --write '**/*.{js,jsx}'",
    "test": "NODE_ENV=test jest --coverage --setupTestFrameworkScriptFile=./jest.setup.js",
    "build": "NODE_ENV=production rollup -c",
    "watch": "rollup -c -w",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public -c .storybook -o docs --docs",
    "prepublishOnly": "rm -rf ./dist && npm run build",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@chakra-ui/react": "^1.7.2",
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addons": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@storybook/theming": "^6.3.12",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "autoprefixer": "^10.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "babel-preset-react-app": "^10.0.0",
    "chance": "^1.1.8",
    "core-js": "^3.19.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.25.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-trigo": "^21.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-compat": "^4.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "final-form": "^4.20.4",
    "final-form-arrays": "^3.0.2",
    "framer-motion": "^5.3.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.2",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.7",
    "react-final-form-arrays": "3.1.3",
    "react-intl": "^5.21.2",
    "regenerator-runtime": "^0.13.9",
    "rollup": "^2.60.0",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^1.6.0",
    "@emotion/react": "^11",
    "core-js": "^3.11.1",
    "date-fns": "^2.21.1",
    "final-form": "^4.20.2",
    "final-form-arrays": "^3",
    "framer-motion": "^4.1.11",
    "prop-types": "^15",
    "react": "^17.0.2",
    "react-final-form": "^6.5.3",
    "react-final-form-arrays": "^3",
    "react-intl": "^5.17.4"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)(spec|test|specs).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/?(*.)(stories).js?(x)"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^1.0.5",
    "@storybook/addon-actions": "^6.3.12",
    "react-table": "^7.7.0"
  }
}
