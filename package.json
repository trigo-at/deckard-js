{
  "name": "@trigo/design-system",
  "version": "0.13.10",
  "description": "trigo design system",
  "main": "dist/designsystem.cjs.js",
  "module": "dist/designsystem.esm.js",
  "sideEffects": false,
  "repository": "git@github.com:trigo-at/design-system.git",
  "author": "trigo GmbH <office@trigo.at>",
  "license": "MIT",
  "engines": {
    "node": ">=9.0.0"
  },
  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "prettify": "prettier --write 'src/**/*.{js,jsx}'",
    "test": "NODE_ENV=test jest --coverage --setupTestFrameworkScriptFile=./jest.setup.js",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "lint": "eslint .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o docs",
    "prepare": "rm -rf ./dist && yarn build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
      "eslint .",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "@storybook/addon-actions": "^4.1.9",
    "@storybook/addon-info": "^4.1.9",
    "@storybook/addon-links": "^4.1.9",
    "@storybook/addon-options": "^4.1.9",
    "@storybook/addons": "^4.1.9",
    "@storybook/react": "^4.1.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "camelcase": "^5.0.0",
    "cross-env": "^5.2.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-react-trigo": "^6.3.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "file-loader": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-test-renderer": "^16.7.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "styled-components": "^4.1.3",
    "styled-system": "^3.2.1",
    "svgi": "^1.1.0"
  },
  "peerDependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "styled-components": "^3.2.6",
    "styled-system": "^2.2.5"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)(spec|test|specs).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/?(*.)(stories).js?(x)"
    ]
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
